= form_with url: import_path, method: :post do |f|
  = f.file_field :file, accept: '.csv'
  = f.submit 'dsfsdf', class: 'btn btn-primary'

table.table.table-sm 
  thead
    tr
      - Transaction.column_names.each do |column_name|
        th = column_name
  tbody
    - @transactions.each do |transaction|
      tr
        - transaction.attributes.values.each do |value|
          td = value
